import numpy as np
import matplotlib.pyplot as plt

# a) Plotting the data points:
    
a=np.loadtxt('population.csv',delimiter=',')
x=a[:,0]
f=a[:,1]

X = np.array(x)
Y = np.array(f)
plt.figure(1)
plt.plot(X,Y)
plt.title("Plot of csv data")

